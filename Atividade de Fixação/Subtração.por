//Subtração
//Solicitar o usuário 2 números para realizar a subtração
//Utilizar o "for" para realizar a operação 3 vezes

programa
{
	inteiro num1, num2
	
	funcao inicio()
	{
		para(inteiro i=0; i<=2; i++){
			escreva("Informe dois números para realizar a subtração: \n"
			)
			leia(num1, num2)
			escreva("Resultado: ", num1 - num2, "\n\n")
			}
	}
}
