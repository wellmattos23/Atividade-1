//Números Ímpares
//solicitar número do usuário
//utilizar o "for" para verificar todos o números até N
//imprimir ao usuário todos os números ímpares até N

programa
{
	inteiro num
	
	funcao inicio()
	{
		escreva("Informe um número: ")
		leia(num)
		para(inteiro i=1; i<=num; i+=2){
			escreva(i,"\n")
			}
	}
}
